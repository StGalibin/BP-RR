Процедура ЗаменитьТипыСкладовВРегистреСчетовУчетаНоменклатуры() Экспорт
	Запрос = Новый Запрос;
	Запрос.Текст =
	"ВЫБРАТЬ
	|	*
	|ИЗ
	|	РегистрСведений.СчетаУчетаНоменклатуры КАК СчетаУчетаНоменклатуры";
	
	ОтборПоИзмерениям = Новый Структура;
	Для каждого Измерение Из Метаданные.РегистрыСведений.СчетаУчетаНоменклатуры.Измерения Цикл
		ОтборПоИзмерениям.Вставить(Измерение.Имя);
	КонецЦикла;

	ТаблицаРезультата = Запрос.Выполнить().Выгрузить();
	Инд = ТаблицаРезультата.Количество()-1;
	Пока Инд >= 0 Цикл
		Строка = ТаблицаРезультата[Инд];
		Инд = Инд - 1;
		Если ЗначениеЗаполнено(Строка.ТипСклада) Тогда
			Продолжить;
		ИначеЕсли ЗначениеЗаполнено(Строка.Склад) Тогда
			Строка.ТипСклада = Строка.Склад.ТипСклада;
		ИначеЕсли Не ЗначениеЗаполнено(Строка.УдалитьТипСклада) Тогда
			Продолжить;
		ИначеЕсли Строка.УдалитьТипСклада = Перечисления.УдалитьТипыСкладов.Оптовый Тогда
			Строка.ТипСклада = Перечисления.ТипыСкладов.ОптовыйСклад;
		ИначеЕсли Строка.УдалитьТипСклада = Перечисления.УдалитьТипыСкладов.Розничный Тогда
			ЗаполнитьЗначенияСвойств(ОтборПоИзмерениям, Строка);
			ОтборПоИзмерениям.ТипСклада = Перечисления.ТипыСкладов.НеавтоматизированнаяТорговаяТочка;
			Если ТаблицаРезультата.НайтиСтроки(ОтборПоИзмерениям).Количество() = 0 Тогда
				НоваяСтрока = ТаблицаРезультата.Добавить();
				ЗаполнитьЗначенияСвойств(НоваяСтрока, Строка,,"УдалитьТипСклада");
				НоваяСтрока.ТипСклада = Перечисления.ТипыСкладов.НеавтоматизированнаяТорговаяТочка;
			КонецЕсли;
			Строка.ТипСклада = Перечисления.ТипыСкладов.РозничныйМагазин;
		КонецЕсли;
		Строка.УдалитьТипСклада = Перечисления.ТипыСкладов.ПустаяСсылка();
		ЗаполнитьЗначенияСвойств(ОтборПоИзмерениям, Строка);
		Если ТаблицаРезультата.НайтиСтроки(ОтборПоИзмерениям).Количество() > 1 Тогда
			ТаблицаРезультата.Удалить(Строка);
		КонецЕсли;
	КонецЦикла;

	НаборЗаписей = РегистрыСведений.СчетаУчетаНоменклатуры.СоздатьНаборЗаписей();
	НаборЗаписей.Загрузить(ТаблицаРезультата);
	НаборЗаписей.Записать(Истина);
КонецПроцедуры

Процедура ОчиститьТиповыеОперации20Отложенно(Параметры) Экспорт

	// Типовые операции обычно содержат немного записей, поэтому все их очищаем за одну итерацию.
	Запрос = Новый Запрос;
	
	Запрос.Текст =
	"ВЫБРАТЬ
	|	УдалитьТиповыеОперации.Ссылка
	|ИЗ
	|	Справочник.УдалитьТиповыеОперации КАК УдалитьТиповыеОперации";

	УстановитьПривилегированныйРежим(Истина);

	РезультатЗапроса = Запрос.Выполнить();

	Если РезультатЗапроса.Пустой() Тогда
		Параметры.ОбработкаЗавершена = Истина;
		Возврат;
	КонецЕсли;
	
	Параметры.ОбработкаЗавершена = Ложь;
	
	ОбъектовОбработано = 0;
	ПроблемныхОбъектов = 0;
	
	Выборка = РезультатЗапроса.Выбрать();
	
	Пока Выборка.Следующий() Цикл
		
		Попытка
		
			СпрОбъект = Выборка.Ссылка.ПолучитьОбъект();
			Если СпрОбъект = Неопределено Тогда
				// Уже удален, пропускаем.
				Продолжить;
			КонецЕсли;	
			
			// Физически удаляем объект из базы.
			СпрОбъект.ОбменДанными.Загрузка = Истина;
			СпрОбъект.Удалить();
		
			ОбъектовОбработано = ОбъектовОбработано + 1;
			
		Исключение
			// Если не удалось обработать какой-либо документ, повторяем попытку снова.
			ПроблемныхОбъектов = ПроблемныхОбъектов + 1;
			
			ТекстСообщения = СтрШаблон(
				НСтр("ru = 'Не удалось удалить %1 по причине:
					|%2'", ОбщегоНазначенияКлиентСервер.КодОсновногоЯзыка()),
				Выборка.Ссылка,
				ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));

			ЗаписьЖурналаРегистрации(
				ОбновлениеИнформационнойБазы.СобытиеЖурналаРегистрации(),
				УровеньЖурналаРегистрации.Предупреждение,
				Метаданные.Справочники.УдалитьТиповыеОперации,
				Выборка.Ссылка, 
				ТекстСообщения);
		КонецПопытки;
		
	КонецЦикла;
	
	Если ПроблемныхОбъектов = 0 Тогда
		Параметры.ОбработкаЗавершена = Истина;
	КонецЕсли;
	
	Если ОбъектовОбработано = 0 Тогда
		ТекстСообщения = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
			НСтр("ru = 'Процедуре ОбновлениеИнформационнойБазыБП.ОчиститьТиповыеОперации20Отложенно()
				|не удалось удалить некоторые ссылки (пропущены): %1'"),
				ПроблемныхОбъектов);
		ВызватьИсключение ТекстСообщения;
	Иначе
		ЗаписьЖурналаРегистрации(
			ОбновлениеИнформационнойБазы.СобытиеЖурналаРегистрации(),
			УровеньЖурналаРегистрации.Информация,
			Метаданные.Справочники.УдалитьТиповыеОперации,
			,
			СтрШаблон(НСтр("ru = 'Процедура ОбновлениеИнформационнойБазыБП.ОчиститьТиповыеОперации20Отложенно()
					|обработала очередную порцию: %1 элементов справочника'", ОбщегоНазначенияКлиентСервер.КодОсновногоЯзыка()),
					ОбъектовОбработано));
	КонецЕсли;

КонецПроцедуры

