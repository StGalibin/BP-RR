				Если ИерархияРодителей.Количество() > 0 Тогда
					Регион = ИерархияРодителей[ИерархияРодителей.ВГраница()];
				КонецЕсли;
			КонецЕсли;
			Объект.Родитель = Регион;
		КонецЕсли;
	КонецЕсли;
	
	Объект.Наименование    = ОбъектXDTO.Наименование;
